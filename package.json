{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "React boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "start": "cross-env NODE_ENV=development node util/devServer.js",
    "serve:prod": "cross-env NODE_ENV=production node util/staticServer.js",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --progress --colors",
    "postbuild": "npm run serve:prod",
    "lint": "eslint app/js util/devServer.js webpack.config.dev.js webpack.config.prod.js"
  },
  "author": "Chen Yu",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "copy-webpack-plugin": "^2.1.3",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.0.1",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "image-webpack-loader": "^1.8.0",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  }
}
