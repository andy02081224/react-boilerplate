{	
	"parser": "babel-eslint",
	"parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
	},
	"env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true,
    "mocha": true
   },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
  	"no-console": 0,
  	"no-unused-vars": 1
  },
  "plugins": [
    "react"
  ]
}